        "@types/jasmine": "~2.8.6",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "~8.9.4",
        "body-parser": "^1.18.3",
        "codelyzer": "~4.2.1",
        "express": "^4.16.4",
        "faker": "^4.1.0",
        "gulp": "^4.0.0",
        "gulp-inject": "^4.3.2",
        "gulp-rename": "^1.2.3",
        "gulp-svgmin": "^1.2.4",
        "gulp-svgstore": "^6.1.1",
        "jasmine-core": "~2.99.1",
        "jasmine-spec-reporter": "~4.2.1",
        "json-server": "0.14.0",
        "karma": "~1.7.1",
