{
  "name": "angular-cli-project",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "start:proxy": "ng serve --proxy-config proxy.conf.json --open",
    "hmr": "ng serve --configuration hmr --open --port 4201",
    "db": "json-server --port 3004 --watch db/db.js",
    "server": "tsc-watch ./server/server.ts --outDir ./server/ --onSuccess \"node ./server/server.js\" --onFailure \"echo Beep! Compilation Failed\" --compiler typescript/bin/tsc",
    "build": "ng build --prod",
    "build+": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --prod",
    "prerender": "ng run angular-cli-project:prerender",
    "build:serve": "cd ./dist && ws --spa index.html --compress --open --static.maxage -1",
    "build:serve+": "webpack-dev-server --open --entry /webpack-dev-server --output-path /dist --compress --content-base ./dist --history-api-fallback",
    "svg": "gulp svg",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "report": "webpack-bundle-analyzer dist/stats-es2015.json",
    "analyze": "source-map-explorer 'dist/*.js'",
    "ts": "tsc-watch --onSuccess \"node ./src/app/modules/typescript/plain/dist/typescript-plain.js\"  --onFailure \"echo Beep! Compilation Failed\" --compiler typescript/bin/tsc -p ./src/app/modules/typescript/plain/",
    "ts:serve": "cd ./src/app/modules/typescript/plain && ws --spa index.html --compress --open --static.maxage -1",
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
    "dev:ssr": "ng run angular-cli-project:serve-ssr",
    "serve:ssr": "node dist/angular-cli-project/server/main.js",
    "build:ssr": "ng build --prod && ng run angular-cli-project:server:production"
  },
  "browserslist": [
    "last 2 versions",
    "IE 10",
    "IE 11"
  ],
  "dependencies": {
    "@angular/animations": "^9.1.2",
    "@angular/common": "^9.1.2",
    "@angular/compiler": "^9.1.2",
    "@angular/core": "^9.1.2",
    "@angular/forms": "^9.1.2",
    "@angular/platform-browser": "^9.1.2",
    "@angular/platform-browser-dynamic": "^9.1.2",
    "@angular/platform-server": "^9.1.2",
    "@angular/router": "^9.1.2",
    "@angular/service-worker": "^9.1.2",
    "@egjs/hammerjs": "^2.0.17",
    "@herodevs/hero-loader": "^2.0.1",
    "@ngrx/effects": "^9.1.0",
    "@ngrx/router-store": "^9.1.0",
    "@ngrx/store": "^9.1.0",
    "@nguniversal/express-engine": "^9.1.1",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@sendgrid/mail": "^6.5.5",
    "@types/multer": "^1.4.3",
    "classlist.js": "^1.1.20150312",
    "compression": "^1.7.4",
    "core-js": "^2.5.4",
    "dicer": "^0.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.15.2",
    "hammerjs": "^2.0.8",
    "highcharts": "^6.1.0",
    "install": "^0.12.1",
    "jasmine-marbles": "^0.4.1",
    "jsonwebtoken": "^8.5.1",
    "less-loader": "^5.0.0",
    "npm": "^6.2.0",
    "resize-observer-polyfill": "^1.5.1",
    "rxjs": "^6.5.3",
    "smoothscroll-polyfill": "^0.4.4",
    "source-map-explorer": "^1.7.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^9.1.0",
    "@angular-devkit/build-angular": "~0.901.6",
    "@angular/cli": "^9.1.6",
    "@angular/compiler-cli": "^9.1.2",
    "@angular/language-service": "^9.1.2",
    "@angularclass/hmr": "^2.1.3",
    "@ngrx/store-devtools": "^8.3.0",
    "@nguniversal/builders": "^9.1.1",
    "@types/express": "^4.16.0",
    "@types/hammerjs": "^2.0.35",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "body-parser": "^1.18.3",
    "buffer": "^5.6.0",
    "codelyzer": "^5.1.2",
    "compression-webpack-plugin": "^2.0.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "faker": "^4.1.0",
    "gulp": "^4.0.0",
    "gulp-inject": "^4.3.2",
    "gulp-rename": "^1.2.3",
    "gulp-svgmin": "^1.2.4",
    "gulp-svgstore": "^6.1.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "json-server": "0.14.0",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "local-web-server": "^2.5.2",
    "lodash": "^4.17.11",
    "multer": "^1.4.1",
    "mz": "^2.7.0",
    "ngrx-store-freeze": "^0.2.4",
    "parse5": "^6.0.0",
    "protractor": "~5.3.0",
    "puppeteer": "^3.0.0",
    "ts-node": "~5.0.1",
    "tsc-watch": "^1.0.30",
    "tslint": "~5.9.1",
    "typescript": "~3.7.5",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.2.3"
  }
}
